{
  "expo": {
    "name": "Baryc",
    "slug": "baryc",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "GermainTL",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Votre position GPS sera utilis√©e pour trouver les bars qui sont le plus proche entre vous et vos amis.",
        "NSAllowsArbitraryLoads": false,
        "LSApplicationQueriesSchemes": ["comgooglemaps", "citymapper", "uber", "map", "geo"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
